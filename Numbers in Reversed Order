using System;


namespace NumbersReversedOrder
{
    class Program
    {
        static void Main(string[] args)
        {

            string input = Console.ReadLine().ToLower();
            string result = ""; 
            
            for (int i = input.Length -1; i >= 0; i--) 
            {
                result += input[i]; // добави 
           
            }

            Console.WriteLine(result);
        }
    }
}
